# function with return:

def operation(a,b):
    return(a*b)
result= operation(4,5)
print(result)
____________________________
 # wap to identify maximum number
def num(x,y):
    return max(x,y)
result=max(13,17)
print('maximum number is :', result)
_____________________________
# wap python program maximum number, multiplication and +100
def num(x,y):
    return max(x,y),x*y,x+100
result = num(3,4)
print( result
_____________________________
# wap python program maximum number, multiplication and +100
def num(n):
    return[i**2 for i in range(n,n+1) ]
result = num(4)
print( result)
_____________________________
# wap to reverse the character 
def ch(d):
    return d[::-1]
result =ch('ibrahim')
print( result)
_______________________________
#positional argument 

 # wap to for area of rectangle 
def a(length,width):
    return (length *width)
area=a(20,30)
print( area)
______________________________

#wap to add character
 def cha(a,b):
    return a +b
result = cha('Ibrahim','Sayyad')
print(result )
