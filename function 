#default argument

#wap to display bank account 
def account(name='guest',branch='HDFC:'):
    print('Name is :',name)
    print('Branch is :', branch)
account(branch='karve_naka',name='ibrahim')
_______________________________________

#wap to display bank account 
def account(name='guest',branch='HDFC:'):
    print('Name is :',name)
    print('Branch is :', branch)
account(branch='karve_naka',name='ibrahim')
print('_______________')
account ('ibrahim')
print('_______________')
account(branch='karve_naka')
______________________________________

 # wap to for area of triangle 
def a(length,width):
    print(' Length of triangle ',length )
    print ('Width of triangle', width )
    print('Area of triangle ',length*width)
a(3,4)
print('--------------')
a(width=6,length=4)
___________________________________

# wap to to display  numbers 
def a(b,c):
    print('Addition of numbers:',b+c)
    print('substraction of numbers:',b-c)
   
a(15,12)
print('-----------------')
a(c=3,b=4)
______________________________________

#variable length args
#1positional var len arg 

# wap to to display  numbers 
def a(*b):
    print(b)
   
a(15,12)
print('-----------------')
a()
a(22,'ibrahim',2001)
______________________________

# wap to to display  numbers 
def a(*b):
    print(sum(b))
   
a(15,12)
print('-----------------')
a()
a(22,2001)
______________________________
 # wap to to display  numbers 
def a(*b):
    print(len(b))
   
a(15,12,45,12)
print('-----------------')
a('ibrahim')
a(22,2001)
__________________________________