#wap to display birthdate 
 def cha(date,month,year):
    print('date:',date )
    print('month:',month)
    print('yeae :',year)
cha(12,'feb',2024)
_________________________________

 #write a program to name bikes 
def cha(bike1, bike2, bike3):
    print('yamaha:',bike1)
    print('kawasaki:',bike2)
    print('Benelli:',bike3)
cha('R15','ninja','600i')
___________________________________
#keyword Argument

 #write a program to name bikes 
def cha(bike1, bike2, bike3):
    print('yamaha:',bike1)
    print('kawasaki:',bike2)
    print('Benelli:',bike3)
cha(bike2='ninja',bike3='600i',bike1='R15')
____________________________________
 #write a program for students information 
def student_info(student1, student2, student3):
    print('rahul:', student1)
    print('ramesh:', student2)
    print('suresh:', student3)
student_info(student2='(Roll no:5,Grade:A)' , student3='(Roll no:6,Grade:B)', student1='(Roll no:4,Grade:C)')
____________________________________

#wap to display full name with age 
def full_name(fs,ls,ag):
    print('first_name',fs)
    print ('last_name',ls)
    print('Age',ag)
full_name(ls='sayyad',fs='ibrahim',ag=22)
________________________________________

#wap to display full name with age 
def num(s,l):
    print('addition:',s+l)
    print ('substraction:',s-l)
num(l=20,s=25)
__________________________________________#wap to display area of rectangle
def rect(l,w):
    print('area of rectangle:',l*w )
rect(l=3,w=5)
