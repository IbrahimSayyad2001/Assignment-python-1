class grand_parents:
    def property(self  ):
        print('property from grand_parents')
class mother(grand_parents):
    def money(self):
        print('Money from Mother')
        super().property()
class father(grand_parents):
    def money(self):
         print('FATHERS MONEY')

    def car(self):
        print(' fathers car')
class child(mother,father):
    pass
c=child()
c.money()
c.car()
c.property()
