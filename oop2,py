class bikes():
      def __init__(self,company,model,year_of_manufacture,price):
          self.company = company
          self.model = model
          self.year_of_manufacture = year_of_manufacture
          self.price = price
class yamaha (bikes):
    def spec(self):
        print('company name',self.company)
        print('model',self.model)
        print('year_of_manufacture',self.year_of_manufacture)
        print(' price of bike',self.price)
class honda (bikes):
    def spec(self):
        print('company name', self.company)
        print('model', self.model)
        print('year_of_manufacture', self.year_of_manufacture)
        print(' price of bike', self.price)

