i=0
j=5
for row in range(6):
    for col in range(6):
        if  (col==row)   :
            print('*',end=' ')
        elif (row==i and col==j):
            print('*', end=' ')
            i=i+1
            j=j-1
   
        else:
            print(end='  ')
    print ()
