i=0
j=4
for row in range(5):
    for col in range(5):
        if  (col==row and col<3)or (col==2 and (row>1))  :
            print('*',end=' ')
        elif (row==i and col==j and col>2)  :
            print('*', end=' ')
            i=i+1
            j=j-1
   
        else:
            print(end='  ')
    print ()
_____________________________

for row in range(6):
    for col in range(6):
        if  (row==0) or (row==5) or (col==4 and (row==1)) or (col==3 and (row==2)) or (col==2 and (row==3)) or (col==1 and (row==4))  :
            print('*',end=' ')
   
        else:
            print(end='  ')
    print ()
