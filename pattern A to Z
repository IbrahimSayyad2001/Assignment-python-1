for row in range(7):
    for col in range(5):
        if col==0 or (col==4 and (row!=1 and row!=2)) or (row==0 or row==6) and  (col>0 and col<4) or (row==3 and (col==3 or col==5)):
            print('*',end=' ')
        else:
            print(end='  ')
    print ()
