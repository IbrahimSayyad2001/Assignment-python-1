 for row in range(7):
    for col in range(5):
        if (col ==0)or (col ==4 and (row!=0 and row!=3 and row !=6)) or (row==0 or row==3 or  row==6 ) and (col>0 and col<4) :
            print('*',end=' ')
        else :
            print(end='  ')
    print()
_______________________________________

for i in range(7):
    for j in range(5):
        if (i==0 and j==1 ) or (i==0 and j==2 ) or ((i==0 and j==3) or i==3) or (i>0)and (j==0 or j==4 ):
            print('*',end='')
        else :
            print(end=' ')
    print()
